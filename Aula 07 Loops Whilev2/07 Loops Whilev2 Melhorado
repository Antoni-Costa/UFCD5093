#include <stdio.h>

int main(void) {

    int fact;
    int num;


    printf("Enter a positive number: \n");
    scanf("%d", &num);

    fact = 1;
    num = num;
        while (num > 0) {
            fact = fact * num;
            num -= 1;
        }
        printf("Factorial of %d is %d\n", num, fact);

    return 0;
}
